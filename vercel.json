{
    "builds": [{
        "src": "JAM_AGENT_NOTIFICATION_TRACKER/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.12"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "JAM_AGENT_NOTIFICATION_TRACKER/wsgi.py"
        }
    ],
    "functions": {
        "api/*.py": {
            "maxLambdaSize": "15mb"
        }
    }
}
